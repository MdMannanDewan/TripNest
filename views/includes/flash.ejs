<% if(successMsg && successMsg.length){ %>
<div
  class="alert alert-success alert-dismissible fade show col-6 mx-auto"
  role="alert"
>
  <%= successMsg %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>

<% } %> <% if(errorMsg && errorMsg.length){ %>
<div
  class="alert alert-success alert-dismissible fade show col-6 mx-auto"
  role="alert"
>
  <%= errorMsg %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>

<% } %>
